{
  "name": "bookmark-tools",
  "displayName": "__MSG_extensionName__",
  "version": "0.0.1",
  "description": "__MSG_extensionDescription__",
  "author": "kylee0325",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "antd": "^5.19.1",
    "dayjs": "^1.11.11",
    "plasmo": "0.88.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "name": "__MSG_extensionName__",
    "action": {
      "default_title": "Click to open panel"
    },
    "default_locale": "zh_CN",
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "tabs",
      "scripting",
      "bookmarks",
      "history",
      "favicon",
      "contextMenus"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "_favicon/*"
        ],
        "matches": [
          "<all_urls>"
        ],
        "extension_ids": [
          "*"
        ]
      }
    ]
  }
}